{
  "expo": {
    "name": "pomodoggo",
    "slug": "pomodoggo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/marylois.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A pomodoro timer that also fetches random dog photos.",
    "githubUrl": "https://github.com/bgaudino/pomodoggo-mobile.git"
  }
}
